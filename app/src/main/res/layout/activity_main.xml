<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/scoreTextView"
        android:layout_width="133dp"
        android:layout_height="30dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:gravity="center_vertical"
        android:text="Score: 0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/timetextView"
        android:layout_width="108dp"
        android:layout_height="26dp"
        android:layout_marginStart="40dp"
        android:text="60 Seconds Left"
        app:layout_constraintBottom_toBottomOf="@+id/scoreTextView"
        app:layout_constraintStart_toEndOf="@+id/scoreTextView"
        app:layout_constraintTop_toTopOf="@+id/scoreTextView" />

    <ImageView
        android:id="@+id/mole1"
        android:layout_width="360px"
        android:layout_height="360px"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="30dp"
        android:scaleType="matrix"
        app:layout_constraintBottom_toTopOf="@+id/mole5"
        app:layout_constraintEnd_toStartOf="@+id/mole5"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/mole2"
        android:layout_width="360px"
        android:layout_height="360px"
        android:layout_marginBottom="30dp"
        android:scaleType="matrix"
        app:layout_constraintBottom_toTopOf="@+id/mole5"
        app:layout_constraintEnd_toEndOf="@+id/mole5"
        app:layout_constraintStart_toStartOf="@+id/mole5"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/mole3"
        android:layout_width="360px"
        android:layout_height="360px"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="30dp"
        android:scaleType="matrix"
        app:layout_constraintBottom_toTopOf="@+id/mole5"
        app:layout_constraintStart_toEndOf="@+id/mole5"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/mole4"
        android:layout_width="360px"
        android:layout_height="360px"
        android:layout_marginEnd="8dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="@+id/mole5"
        app:layout_constraintEnd_toStartOf="@+id/mole5"
        app:layout_constraintTop_toTopOf="@+id/mole5"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/mole5"
        android:layout_width="360px"
        android:layout_height="360px"
        android:scaleType="matrix"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/scoreTextView"
        app:layout_constraintVertical_bias="0.39"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/mole6"
        android:layout_width="360px"
        android:layout_height="360px"
        android:layout_marginStart="8dp"
        android:scaleType="matrix"
        app:layout_constraintBottom_toBottomOf="@+id/mole5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/mole5"
        app:layout_constraintTop_toTopOf="@+id/mole5"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/mole7"
        android:layout_width="360px"
        android:layout_height="360px"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="8dp"
        android:scaleType="matrix"
        app:layout_constraintEnd_toStartOf="@+id/mole5"
        app:layout_constraintTop_toBottomOf="@+id/mole5"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/mole8"
        android:layout_width="360px"
        android:layout_height="360px"
        android:layout_marginTop="30dp"
        android:scaleType="matrix"
        app:layout_constraintEnd_toEndOf="@+id/mole5"
        app:layout_constraintStart_toStartOf="@+id/mole5"
        app:layout_constraintTop_toBottomOf="@+id/mole5"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/mole9"
        android:layout_width="360px"
        android:layout_height="360px"
        android:layout_marginStart="8dp"
        android:layout_marginTop="30dp"
        android:scaleType="matrix"
        app:layout_constraintStart_toEndOf="@+id/mole5"
        app:layout_constraintTop_toBottomOf="@+id/mole5"
        tools:srcCompat="@tools:sample/avatars" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="362dp"
        android:layout_height="54dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mole8"
        app:layout_constraintVertical_bias="0.100000024">

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>